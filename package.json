{
   "name": "etap",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "dotenv -e .env.test -- db:test:restart jest --watch --no-cache --config ./test/jest-e2e.json",
      "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 1 && yarn migrate:deploy",
      "db:dev:up": "docker compose up dev-db -d",
      "db:dev:rm": "docker compose rm dev-db -s -f -v",
      "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 1 && yarn test:migrate:deploy",
      "db:test:up": "docker compose up test-db -d",
      "db:test:rm": "docker compose rm test-db -s -f -v",
      "prisma:migrate:prod": "prisma migrate deploy",
      "prisma:migrate:reset": "prisma migrate reset",
      "prisma:migrate:dev": "prisma migrate dev",
      "start:migrate:dev": "yarn prisma:migrate:dev && yarn start:dev",
      "start:migrate:prod": "yarn prisma:migrate:prod && yarn start:prod"
   },
   "dependencies": {
      "@nestjs/axios": "^1.0.1",
      "@nestjs/cache-manager": "^1.0.0",
      "@nestjs/common": "^10.2.5",
      "@nestjs/config": "^2.2.0",
      "@nestjs/core": "^10.2.5",
      "@nestjs/event-emitter": "^1.3.1",
      "@nestjs/jwt": "^9.0.0",
      "@nestjs/mapped-types": "^2.0.2",
      "@nestjs/passport": "^9.0.0",
      "@nestjs/platform-express": "^9.0.0",
      "@nestjs/platform-socket.io": "^10.2.5",
      "@nestjs/schedule": "^2.1.0",
      "@nestjs/swagger": "^7.1.11",
      "@nestjs/websockets": "^10.2.5",
      "@prisma/client": "4.8.0",
      "@types/multer": "^1.4.7",
      "argon2": "^0.30.1",
      "aws-sdk": "^2.1253.0",
      "axios": "^1.2.2",
      "cache-manager": "^5.1.4",
      "cache-manager-redis-store": "^3.0.1",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "cuid": "^2.1.8",
      "dotenv-cli": "^6.0.0",
      "js-sha256": "^0.9.0",
      "nestjs-prisma": "^0.20.0",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.0",
      "qrcode": "^1.5.3",
      "redis": "^4.5.1",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.2.0"
   },
   "devDependencies": {
      "@nestjs/cli": "^9.0.0",
      "@nestjs/schematics": "^9.0.0",
      "@nestjs/testing": "^9.0.0",
      "@types/express": "^4.17.13",
      "@types/jest": "28.1.8",
      "@types/node": "^16.0.0",
      "@types/passport-jwt": "^3.0.7",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "28.1.3",
      "pactum": "^3.2.4",
      "prettier": "^2.3.2",
      "prisma": "4.8.0",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "28.0.8",
      "ts-loader": "^9.2.3",
      "ts-node": "^10.0.0",
      "tsconfig-paths": "4.1.0",
      "typescript": "^4.7.4"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node",
      "moduleNameMapper": {
         "^@etap-app/(.*)$": "<rootDir>/../src/app/$1",
         "^@etap-config/(.*)$": "<rootDir>/../src/config/$1",
         "^@etap-utils/(.*)$": "<rootDir>/../src/utility/$1"
      }
   },
   "resolutions": {
      "@nestjs/common": "^10.2.5",
      "@nestjs/core": "^10.2.5"
   },
   "overrides": {
      "@nestjs/common": "^10.2.5",
      "@nestjs/core": "^10.2.5"
   }
}
